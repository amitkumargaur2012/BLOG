
    <%- include header%>
  <% posts.forEach(function(post, index) {%>
    <h2><a href="/u/<%=post.name%>/<%=post.time.day%>/<%=post.title%>"><%= post.title%></a> </h2>
            <p class="info">
                Author:<a href="/u/<%=post.name%>"><%= post.name%></a> |
                Date:<%=post.time.minute%>
            </p>
            <p class="content"><%- post.post%></p>
            <hr>
  <%})%>

  <%- include footer%>
